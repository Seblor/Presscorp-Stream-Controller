{
  "name": "presscorp-stream-controller",
  "description": "Presscorp Stream Controller",
  "license": "MIT",
  "private": true,
  "version": "0.1.0",
  "author": {
    "name": "Seblor"
  },
  "type": "module",
  "build": {
    "iconPath": "public/favicon.png",
    "productName": "Presscorp Stream Controller"
  },
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "concurrently --kill-others \"vite\" \"npm run dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "cross-env DEV_ENV=true electron .",
    "build": "vite build",
    "preview": "vite preview",
    "package": "npm run build && electron-forge package",
    "make": "npm run build && electron-forge make"
  },
  "devDependencies": {
    "@electron-forge/cli": "7.4.0",
    "@electron-forge/maker-deb": "7.4.0",
    "@electron-forge/maker-rpm": "7.4.0",
    "@electron-forge/maker-squirrel": "7.4.0",
    "@electron-forge/maker-zip": "7.4.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@types/node": "^22.8.7",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "electron": "^33.0.2",
    "electron-reload": "^2.0.0-alpha.1",
    "postcss": "^8.4.47",
    "svelte": "^5.1.9",
    "tailwindcss": "^3.4.14",
    "vite": "^5.4.10",
    "zlib-sync": "^0.1.9"
  },
  "dependencies": {
    "discord.js": "^14.16.3",
    "electron-squirrel-startup": "1.0.0"
  }
}
